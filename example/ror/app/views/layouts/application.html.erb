<!DOCTYPE html>
<html>
<head>
  <title>Ror</title>
  <%= stylesheet_link_tag 'app/application' %>
  <%= javascript_include_tag :jquery %>

  <%= javascript_include_tag 'vendor/plupload/plupload.full', 'vendor/jquery.i18n' %>
  <%= javascript_include_tag 'vendor/plupload-ui/jquery.plupload.ui', 'vendor/plupload-ui/i18n/en-US' %>

  <%= stylesheet_link_tag 'vendor/plupload-ui/jquery.plupload.ui.single' %>

  <%= csrf_meta_tag %>
  
  <%= yield :head %>

</head>
<body>

<% flash.each do |name, message| %>
  <div class="<%=name%>"><%= message %></div>
<% end %>

<div class="nav">
  <%= link_to 'Home', root_path %>
  |
  <%= link_to 'Single upload', single_examples_path %>
  |
  <%= link_to 'Multi upload queue', queue_examples_path %>
</div>

<%= yield %>

</body>
</html>
